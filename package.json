{
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "web-build": "expo build:web",
    "test": "jest --watchAll",
    "type-check": "tsc",
    "eslint": "./node_modules/eslint/bin/eslint.js --ext ts,tsx src/*",
    "checks": "yarn eslint && yarn type-check"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/tandemtemplate.app",
        "build": "xcodebuild -workspace ios/TandemRNWebtemplate.xcworkspace  -scheme TandemRNWebtemplate -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11"
        }
      },
      "web": {
        "binaryPath": "http://localhost:5000",
        "type": "detox-puppeteer",
        "device": {
          "defaultViewport": {
            "width": 2560,
            "height": 768
          },
          "headless": false,
          "devtools": false
        },
        "name": "puppeteer"
      }
    }
  },
  "dependencies": {
