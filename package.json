{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test": "jest --watchAll",
        "type-check": "tsc",
        "eslint": "./node_modules/eslint/bin/eslint.js --ext ts,tsx src/*",
        "checks": "yarn eslint && yarn type-check"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "detox": {
        "test-runner": "jest",
        "configurations": {
            "ios": {
                "binaryPath": "e2e/bin/Exponent.app",
                "type": "ios.simulator",
                "name": "iPhone 11"
            },
            "web": { 
                "binaryPath": "http://localhost:5000", 
                "type": "detox-puppeteer",
                "device": {
                  "defaultViewport": {
                    "width": 2560,
                    "height": 768
                  },
                  "headless": false, 
                  "devtools": false
                },
                "name": "puppeteer"
            }
        }
    },
    "dependencies": {
        "@expo/vector-icons": "~10.0.6",
        "@react-native-community/masked-view": "0.1.10",
        "@react-navigation/native": "5.6.1",
        "@react-navigation/stack": "5.6.2",
        "@react-navigation/web": "~1.0.0-alpha.9",
        "expo": "38.0.8",
        "expo-asset": "~8.1.7",
        "expo-constants": "~9.1.1",
        "expo-font": "~8.2.1",
        "expo-linking": "1.0.1",
        "expo-splash-screen": "~0.3.1",
        "expo-status-bar": "1.0.2",
        "expo-web-browser": "~8.3.1",
        "modal-enhanced-react-native-web": "0.2.0",
        "react": "~16.11.0",
        "react-dom": "~16.11.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
        "react-native-gesture-handler": "~1.6.0",
        "react-native-reanimated": "~1.9.0",
        "react-native-safe-area-context": "3.0.7",
        "react-native-screens": "~2.9.0",
        "react-native-web": "~0.11.7",
        "react-redux": "7.2.0",
        "react-test-renderer": "16.13.1",
        "redux": "4.0.5",
        "redux-devtools-extension": "2.13.8",
        "redux-persist": "5.10.0",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "@babel/core": "~7.8.6",
        "@types/jest": "~25.2.1",
        "@types/react": "~16.9.23",
        "@types/react-native": "~0.62.13",
        "@types/react-redux": "~7.1.7",
        "@types/react-test-renderer": "~16.9.2",
        "@types/redux-persist": "~4.3.1",
        "@typescript-eslint/eslint-plugin": "~2.28.0",
        "@typescript-eslint/parser": "~2.28.0",
        "babel-eslint": "10.1.0",
        "babel-preset-expo": "~8.1.0",
        "detox": "~17.6.1",
        "detox-expo-helpers": "~0.6.0",
        "detox-puppeteer": "~2.0.1",
        "eslint": "5.16.0",
        "eslint-config-airbnb": "~18.1.0",
        "eslint-import-resolver-reactnative": "~1.0.2",
        "eslint-plugin-babel": "~5.3.0",
        "eslint-plugin-detox": "~1.0.0",
        "eslint-plugin-import": "~2.20.1",
        "eslint-plugin-jest": "~23.8.2",
        "eslint-plugin-jsx-a11y": "~6.2.3",
        "eslint-plugin-prettier": "~3.1.3",
        "eslint-plugin-react": "~7.19.0",
        "eslint-plugin-react-hooks": "~2.5.0",
        "expo-detox-hook": "~1.0.10",
        "jest-circus": "~26.4.2",
        "jest-expo": "~38.0.0",
        "typescript": "~3.9.5"
    },
    "private": true
}
