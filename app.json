{
  "expo": {
    "name": "Tandem RN+Web template",
    "slug": "tandem-template",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.niktonic21.tandemtemplate"
    },
    "sdkVersion": "38.0.0",
    "android": {
      "package": "com.niktonic21.tandemtemplate"
    }
  }
}